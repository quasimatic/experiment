{
  "name": "glance-dom-selector",
  "version": "0.9.0",
  "description": "Glance DOM selector library",
  "main": "lib/selector.js",
  "scripts": {
    "docs:clean": "rm -rf docs/_book",
    "docs:prepare": "cd docs && gitbook install",
    "docs:watch": "npm run docs:prepare && cd docs && gitbook serve",
    "docs:build": "npm run docs:prepare && cd docs && rm -rf _book && gitbook build",
    "build": "rm -rf lib && node_modules/.bin/pegjs -o src/parser.js src/glance.pegjs && babel src -d lib",
    "test": "echo 'Please test using wallaby'",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quasimatic/glance-selector.git"
  },
  "keywords": [
    "glance",
    "automation",
    "test"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quasimatic/glance-dom-selector-js/issues"
  },
  "homepage": "https://github.com/quasimatic/glance-dom-selector-js#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "eslint": "^3.11.1",
    "gitbook-cli": "^2.3.0",
    "mocha": "^3.2.0",
    "pegjs": "^0.10.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs2-ext": "^0.2.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "wallaby-webpack": "0.0.26",
    "webpack": "^1.13.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
